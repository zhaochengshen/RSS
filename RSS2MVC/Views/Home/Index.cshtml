@{
    ViewBag.Title = "RSS Reader";
}


<div id="myCarousel" class="carousel slide">
    <!-- 轮播（Carousel）指标 -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <!-- 轮播（Carousel）项目 -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="~/Images/sliderpic-1.jpg" alt="测试1">
        </div>
        <div class="item">
            <img src="~/Images/sliderpic-2.jpg" alt="测试2">
        </div>
        <div class="item">
            <img src="~/Images/sliderpic-3.jpg" alt="测试3">
        </div>
        <div class="item">
            <img src="~/Images/sliderpic-4.jpg" alt="测试3">
        </div>
    </div>
    <!-- 轮播（Carousel）导航 -->
    <a class="carousel-control" href="#myCarousel"
       data-slide="prev"></a>
    <a class="carousel-control" style="left:auto;right:0" href="#myCarousel"
       data-slide="next"></a>
</div> 